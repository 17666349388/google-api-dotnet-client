  },
  "rootUrl": "https://abusiveexperiencereport.googleapis.com/",
  "ownerDomain": "google.com",
  "name": "abusiveexperiencereport",
  "batchPath": "batch",
  "fullyEncodeReservedExpansion": true,
  "title": "Abusive Experience Report API"
}
